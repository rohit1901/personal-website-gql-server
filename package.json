{
  "name": "personal-website-gql-server",
  "version": "1.0.0",
  "description": "A GQL Server for my Website",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "compile": "tsc",
    "start": "npm run compile && node ./dist/src/index.js",
    "generate": "graphql-codegen",
    "dev": "run-p dev:*",
    "dev:tsc": "tsc --watch --preserveWatchOutput",
    "dev:node": "node --watch ./dist/src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rohit1901/personal-website-gql-server.git"
  },
  "keywords": [
    "node",
    "javascript",
    "typescript",
    "server",
    "graphql",
    "json-graphql-server"
  ],
  "author": "Rohit Khanduri",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rohit1901/personal-website-gql-server/issues"
  },
  "homepage": "https://github.com/rohit1901/personal-website-gql-server#readme",
  "type": "module",
  "dependencies": {
    "@apollo/server": "^4.10.2",
    "dotenv": "^16.4.5",
    "graphql": "^16.8.1",
    "mongodb": "^6.5.0",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typescript": "^4.0.6",
    "@graphql-codegen/typescript-resolvers": "^4.0.6",
    "@types/node": "^20.12.7",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.4.5"
  }
}
